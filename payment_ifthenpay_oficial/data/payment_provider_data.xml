<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="payment_provider_ifthenpay" model="payment.provider"> 
            <field name="name">ifthenpay gateway</field>
            <field name="code">ifthenpay</field> 
            <field name="company_id" ref="base.main_company"/>
            <field name="state">disabled</field>
            <field name="website_id" ref="website.default_website"/>
            <field name="image_128" type="base64" file="payment_ifthenpay_oficial/static/src/img/ifthenpay.png"/>
            <field name="payment_method_ids"
               eval="[Command.set([
                         ref('payment_ifthenpay_oficial.payment_method_ifthenpay_provider'),
                     ])]"
            />
        </record>
    </data>
</odoo>